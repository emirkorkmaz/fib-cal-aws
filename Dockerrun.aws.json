{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "jaqenhghar2402/fib-cal-aws-client",
            "hostname": "client",
            "essential": false,
            "memory": 1024
        },
        {
            "name": "server",
            "image": "jaqenhghar2402/fib-cal-aws-server",
            "hostname": "api",
            "essential": false,
            "memory": 512
        },
        {
            "name": "worker",
            "image": "jaqenhghar2402/fib-cal-aws-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 512
        },
        {
            "name": "nginx",
            "image": "jaqenhghar2402/fib-cal-aws-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 512
        }
    ]
}